<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.votaciones">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".GraficaActivity">
        </activity>
        <activity
            android:name=".CargaActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PropuestasActivity" />
        <activity android:name=".IntegrantesActivity" />
        <activity android:name=".VotarActivity" />
        <activity android:name=".InicioActivity" />
        <activity android:name=".UsuarioActivity" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>